.list-one{
  height: 160rpx;
  display: flex;
  padding: 0 30rpx;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}
.list-one-poster{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
}
.list-one-right{
  width: 70%;
  margin: 0 0 0 30rpx;
}
.list-one-choose{
  /* background-color: rgba(54, 43, 41, 0.6); */
  color: #E23B34;
}
.list-index{
  display: block;
  width: 60rpx;
  height: 60rpx;
  background-color: #c2c2c2;
  text-align: center;
  border-radius: 60rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  color: #fff;
}
.list-name {
  font-size: 48rpx;
}
.page-top .list-icon{
  position: absolute;
  width: 68rpx;
  height: 38rpx;
  padding: 20rpx 20rpx 20rpx 8rpx ;
  left: 20rpx;
  top: 60rpx;
  z-index: 101;
}
.line {
  margin: 10rpx auto 0;
  width: 300rpx;
  height: 100rpx;
