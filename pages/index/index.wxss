.custom-class {
  color: #fff !important;
}
van-search{
  height: 64rpx
}
.input-class {
  height: 74rpx;
}
.field-class {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  color: #fff !important;
}
.cancel-class {
}
.faceplate {
  position: fixed;
  left: 0;
  bottom: 0;
  padding:20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  width: 100%;
  height:130rpx;
  background-color: #CF9237;
  /* border-top: 1px solid rgba(0,0,0,.5); */
  box-shadow: 0px -1px 1px #aa7008; 
  z-index: 99999;
}
.circle {
  width: 110rpx;
  height: 110rpx;
}
.circle image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.faceplateText {
  width: 211rpx;
  margin: 7rpx  0 0 30rpx;
  box-sizing: border-box;
  padding-bottom: 5rpx;
  height: 100%;
  line-height: 31rpx;
  font-size: 36rpx;
  color: #834129;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

